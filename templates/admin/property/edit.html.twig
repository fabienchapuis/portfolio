{% extends 'base.html.twig' %}

{% block body %}
<div class="container mt-4">
    <h2>Éditer le Projet</h2>

    {{ form_start(form) }}
    <div class="row">
        <div class="col-md-4">{{ form_row(form.title) }}</div>
        <div class="col-md-4">{{ form_row(form.description) }}</div>
        <div class="col-md-4">{{ form_row(form.url) }}</div>
        <div class="col-md-4">{{ form_row(form.date) }}</div>

    </div>
    {{ form_widget(form) }}
    <button class="btn btn-primary">Éditer</button>
    {{ form_end(form) }}



</div>

{% endblock %}